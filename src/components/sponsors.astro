---
import { getCollection } from "astro:content";
import Pack from "../components/pack.astro";

const sponsors = (await getCollection("sponsors")).map((c) => c.data);

const gold = sponsors.filter((s) => s.sponsoring === "gold") ?? [];
const silver = sponsors.filter((s) => s.sponsoring === "silver") ?? [];
const bronze = sponsors.filter((s) => s.sponsoring === "bronze") ?? [];
---

{gold.length > 0 && <Pack title="Gold" partners={gold} />}
{silver.length > 0 && <Pack title="Silver" partners={silver} />}
{bronze.length > 0 && <Pack title="Bronze" partners={bronze} />}

<div class="sponsors community">
  <h3>Partenaire Freelance et Startup</h3>
  <ul>
    <li class="sponsor">
      <a
        aria-label="Visiter le site de GreenNerds"
        href="https://www.linkedin.com/company/codekaio/?originalSubdomain=fr"
      >
        <img
          src="/img/codekaio.png"
          alt=""
          loading="lazy"
          style="max-width: 90%;"
        />
      </a>
    </li>
  </ul>
</div>

